{
  "name": "landing-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "deadcode": "node scripts/run-deadcode.js default",
    "deadcode:ci": "node scripts/run-deadcode.js ci",
    "deadcode:fix": "node scripts/run-deadcode.js fix",
    "deadcode:check": "node scripts/run-deadcode.js check",
    "deadcode:exports": "ts-prune -p tsconfig.tsprune.json",
    "dup:check": "jscpd src --gitignore --min-lines 5 --threshold 12",
    "dup:all": "npm run dup:check",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@next/third-parties": "^15.3.4",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "lucide-react": "^0.523.0",
    "next": "^15.5.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "knip": "^5.68.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.1",
    "tailwindcss": "^4",
    "ts-prune": "^0.10.3",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}
